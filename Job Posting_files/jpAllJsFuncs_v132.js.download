function growArea(t) {
    a = t.value.split('\n');
    b=1;
    colsval=t.cols;
    for (x=0;x < a.length; x++) {
        if (a[x].length >=colsval) b+= Math.floor(a[x].length/colsval);
    }
    b+= a.length-1;
    if(b<6) {
        if (b > t.rows)
            t.style.height = (b+1)*11+'px';
    }
}

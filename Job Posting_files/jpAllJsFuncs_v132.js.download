function markFieldReadonly(obj,field){
    if(obj.checked){
        field.value = "";
        field.readOnly = true;
    } else {
        field.value = "";
        field.readOnly = false;
    }
}

function checkNSplit(classes)
{
    var arrclass = classes.split(' '), cName = "";
    for(var i=0; i<arrclass.length; i++)
    {
        if(arrclass[i] != 'errfldd')
            cName += arrclass[i] + " ";
    }
    return cName;
}

            var decodedOrigStrippedHTML = html_entity_decode(origStrippedHTML);
            var textHTML = html_entity_decode(origHTML);
            var strippedHTML = strip_tags(textHTML);
            if (decodedOrigStrippedHTML == strippedHTML) {
